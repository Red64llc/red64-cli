{
  "name": "red64-cli",
  "version": "0.8.0",
  "description": "Red64 Flow Orchestrator - Deterministic spec-driven development CLI",
  "type": "module",
  "bin": {
    "red64": "./dist/cli.js"
  },
  "files": [
    "dist",
    "framework"
  ],
  "scripts": {
    "build": "tsc",
    "build:ts": "tsc",
    "dev": "tsx src/cli.tsx",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "spec-driven",
    "workflow",
    "orchestration"
  ],
  "author": {
    "name": "Yacin Bahi",
    "email": "yacin@red64.io",
    "url": "https://red64.io"
  },
  "license": "MIT",
  "homepage": "https://github.com/Red64llc/red64-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Red64llc/red64-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Red64llc/red64-cli/issues"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "date-fns": "^3.6.0",
    "github-markdown-css": "^5.7.0",
    "ink": "^5.0.1",
    "marked": "^14.1.3",
    "meow": "^14.0.0",
    "open": "^11.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "ink-testing-library": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
